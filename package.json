{
  "name": "cross-mobile-test-automation",
  "version": "1.0.0",
  "description": "Cross-platform mobile test automation framework with WebdriverIO and Appium",
  "main": "index.js",
  "scripts": {
    "test": "wdio run ./wdio.conf.js",
    "test:android": "PLATFORM=android wdio run ./wdio.conf.js --spec ./tests/android/specs/terms.spec.js",
    "test:android:usb": "node cli/runTests.js --platform android --target usb",
    "test:android:emulator": "node cli/runTests.js --platform android --target emulator",
    "test:ios:simulator": "node cli/runTests.js --platform ios --target simulator",
    "test:ios:usb": "node cli/runTests.js --platform ios --target usb",
    "test:saucelabs": "node cli/runTests.js --target saucelabs",
    "test:browserstack": "node cli/runTests.js --target browserstack",
    "report": "allure generate reports/allure-results --clean && allure open"
  },
  "keywords": [
    "appium",
    "webdriverio",
    "mobile",
    "testing",
    "automation",
    "android",
    "ios"
  ],
  "author": "",
  "license": "ISC",
